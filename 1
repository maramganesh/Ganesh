#include <bits/stdc++.h>
using namespace std;

int knapSack(int W, vector<int>& profit, vector<int>& weight) {
    int n = profit.size();
    vector<int> dp(W + 1, 0);

    for (int i = 0; i < n; i++) {
        for (int w = W; w >= weight[i]; w--) {
            dp[w] = max(dp[w],
                        profit[i] + dp[w - weight[i]]);
        }
    }

    return dp[W];
}

int main() {
    int W = 4;
    vector<int> profit = {1, 2, 3};
    vector<int> weight = {4, 5, 1};

    cout << knapSack(W, profit, weight);
    return 0;
}
