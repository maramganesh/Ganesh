#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n, m;
    cin >> n >> m;

    int L;
    cin >> L;

    // vertical walls: between (x-1,y) and (x,y)
    bool vw[55][55] = {0}; // vw[x][y]
    // horizontal walls: between (x,y-1) and (x,y)
    bool hw[55][55] = {0}; // hw[x][y]

    for (int i = 0; i < L; i++) {
        int x1, y1, x2, y2;
        cin >> x1 >> y1 >> x2 >> y2;

        if (x1 == x2) { // vertical line
            int x = x1;
            int a = min(y1, y2);
            int b = max(y1, y2);
            for (int y = a; y < b; y++) {
                if (x >= 0 && x <= n && y >= 0 && y < m)
                    vw[x][y] = 1;
            }
        }
        else { // horizontal line
            int y = y1;
            int a = min(x1, x2);
            int b = max(x1, x2);
            for (int x = a; x < b; x++) {
                if (y >= 0 && y <= m && x >= 0 && x < n)
                    hw[x][y] = 1;
            }
        }
    }

    int K;
    cin >> K;

    // BFS to find connected components (gift boxes)
    int comp[55][55];
    memset(comp, -1, sizeof(comp));

    int compCount = 0;

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {

            if (comp[i][j] != -1) continue;

            queue<pair<int,int>> q;
            q.push({i, j});
            comp[i][j] = compCount;

            while (!q.empty()) {
                auto [x, y] = q.front();
                q.pop();

                // Up
                if (x > 0 && !vw[x][y] && comp[x-1][y] == -1) {
                    comp[x-1][y] = compCount;
                    q.push({x-1, y});
                }
                // Down
                if (x < n-1 && !vw[x+1][y] && comp[x+1][y] == -1) {
                    comp[x+1][y] = compCount;
                    q.push({x+1, y});
                }
                // Left
                if (y > 0 && !hw[x][y] && comp[x][y-1] == -1) {
                    comp[x][y-1] = compCount;
                    q.push({x, y-1});
                }
                // Right
                if (y < m-1 && !hw[x][y+1] && comp[x][y+1] == -1) {
                    comp[x][y+1] = compCount;
                    q.push({x, y+1});
                }
            }

            compCount++;
        }
    }

    if (K < 1 || K > compCount) {
        cout << "Impossible";
        return 0;
    }

    int ans = INT_MAX;

    // Fix column range [y0, y1)
    for (int y0 = 0; y0 < m; y0++) {
        for (int y1 = y0 + 1; y1 <= m; y1++) {

            int height = y1 - y0;

            vector<int> freq(compCount, 0);
            int unique = 0;

            int left = 0;

            for (int right = 0; right < n; right++) {

                // Add row 'right'
                for (int y = y0; y < y1; y++) {
                    int id = comp[right][y];
                    if (++freq[id] == 1) unique++;
                }

                while (left <= right && unique >= K) {

                    if (unique == K) {
                        int width = right - left + 1;
                        ans = min(ans, width * height);
                    }

                    // Remove row 'left'
                    for (int y = y0; y < y1; y++) {
                        int id = comp[left][y];
                        if (--freq[id] == 0) unique--;
                    }

                    left++;
                }
            }
        }
    }

    if (ans == INT_MAX) cout << "Impossible";
    else cout << ans;

    return 0;
}
