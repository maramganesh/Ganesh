#include <bits/stdc++.h>
using namespace std;

int longestSubset(vector<string>& arr, int a, int b) {
    // dp[i][j] = maximum subset size with at most i zeros and j ones
    vector<vector<int>> dp(a + 1, vector<int>(b + 1, 0));

    for (string s : arr) {
        int zeros = 0, ones = 0;

        // Count zeros and ones in current string
        for (char c : s) {
            if (c == '0') zeros++;
            else ones++;
        }

        // Update dp table backwards (important for 0/1 knapsack)
        for (int i = a; i >= zeros; i--) {
            for (int j = b; j >= ones; j--) {
                dp[i][j] = max(dp[i][j], 
                               dp[i - zeros][j - ones] + 1);
            }
        }
    }

    return dp[a][b];
}

int main() {
    vector<string> arr1 = {"1", "0", "0001", "10", "111001"};
    cout << longestSubset(arr1, 5, 3) << endl;   // Output: 4

    vector<string> arr2 = {"0", "1", "10"};
    cout << longestSubset(arr2, 1, 1) << endl;   // Output: 2

    return 0;
}
